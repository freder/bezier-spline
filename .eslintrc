{
	"extends": "airbnb-base",

	"rules": {
		"indent": [2, "tab", { 'SwitchCase': 1, 'VariableDeclarator': 1 }],
		"no-tabs": 0,
		"arrow-body-style": 0,
		"arrow-parens": 0,
		"comma-dangle": 0,
		"max-len": 1,
		"no-console": 0,
		"no-extra-boolean-cast": 0,
		"no-shadow": 1,
		"no-multi-assign": 0,
		"no-underscore-dangle": 0,
		"no-unused-vars": 1,
		"no-use-before-define": 1,
		"spaced-comment": 1,
		"space-before-function-paren": 0,
		"space-infix-ops": 1,
		"import/first": 0,
		"import/newline-after-import": 0
	}
}
